<Ui xmlns="http://www.blizzard.com/wow/ui" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

<!-- LOW HEALTH JUNK FRAME -->
  <Frame name="GoH4_LowHealthWarning_Pulser_JunkFrame" parent="UIParent" setAllPoints="true" hidden="false" enableMouse="false">
		<Scripts>
			<OnUpdate>
				GoH4_LowHealthWarning_Pulser_OnUpdate();
			</OnUpdate>
		</Scripts>
  </Frame>



<!-- LOW HEALTH EXAMPLE -->
  <Frame name="HealthExampleFrame" parent="WorldFrame" setAllPoints="true"  hidden="true">
    <Layers>
      <Layer level="BACKGROUND">
        <Texture name="$parentTexture" file="Interface\FullScreenTextures\LowHealth" alphaMode="ADD" />
      </Layer>
    </Layers>
  </Frame>

